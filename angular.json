{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-window": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/ng-window",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "aot": true,
            "baseHref": "/ng-window/",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "*.css",
                "input": "./node_modules/@angular/material/prebuilt-themes",
                "output": "assets/theme"
              },
              {
                "glob": "**/*",
                "input": "./src/web-component/vue/assets",
                "output": "./"
              },
              {
                "glob": "README.md",
                "input": ".",
                "output": "./assets/docs"
              }
            ],
            "styles": [
              "./node_modules/normalize.css/normalize.css",
              "src/styles.scss"
            ],
            "scripts": [
              {
                "input": "node_modules/document-register-element/build/document-register-element.js",
                "inject": true
              },
              {
                "input": "node_modules/@webcomponents/webcomponentsjs/webcomponents-bundle.js",
                "inject": true
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "aot": true,
              "deployUrl": "/ng-window/",
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "commonChunk": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kb"
                }
              ]
            },
            "center-dll": {
              "outputPath": "dist/center-dll",
              "vendorChunk": false,
              "assets": [],
              "styles": [],
              "scripts": [],
              "customWebpackConfig": {
                "path": "./webpack.config.center-dll.ts"
              },
              "tsConfig": "src\\tsconfig.dll.json"
            },
            "center-main": {
              "vendorChunk": false,
              "index": {
                "input": "src/index.center-dll.html",
                "output": "index.html"
              },
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "*.css",
                  "input": "./node_modules/@angular/material/prebuilt-themes",
                  "output": "assets/theme"
                },
                {
                  "glob": "**/*",
                  "input": "./src/web-component/vue/assets",
                  "output": "./"
                },
                {
                  "glob": "README.md",
                  "input": ".",
                  "output": "./assets/docs"
                },
                {
                  "glob": "dll.js",
                  "input": "./dist/center-dll",
                  "output": "./"
                }
              ],
              "customWebpackConfig": {
                "path": "./webpack.config.center-main.ts"
              }
            },
            "center-dll-prod": {
              "outputPath": "dist/center-dll",
              "vendorChunk": false,
              "assets": [],
              "styles": [],
              "scripts": [],
              "customWebpackConfig": {
                "path": "./webpack.config.center-dll.ts"
              },
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "buildOptimizer": true,
              "tsConfig": "src\\tsconfig.dll.json"
            },
            "center-main-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": true,
              "aot": true,
              "deployUrl": "/ng-window/",
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "commonChunk": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kb"
                }
              ],
              "index": {
                "input": "src/index.center-dll.html",
                "output": "index.html"
              },
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "*.css",
                  "input": "./node_modules/@angular/material/prebuilt-themes",
                  "output": "assets/theme"
                },
                {
                  "glob": "**/*",
                  "input": "./src/web-component/vue/assets",
                  "output": "./"
                },
                {
                  "glob": "README.md",
                  "input": ".",
                  "output": "./assets/docs"
                },
                {
                  "glob": "dll.js",
                  "input": "./dist/center-dll",
                  "output": "./"
                }
              ],
              "customWebpackConfig": {
                "path": "./webpack.config.center-main.ts"
              }
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "ng-window:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-window:build:production"
            },
            "center-main": {
              "browserTarget": "ng-window:build:center-main"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-window:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "fix": true,
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-window-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-window:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-window:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "fix": true,
            "format": "prose",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-window",
  "cli": {
    "analytics": false,
    "packageManager": "yarn"
  }
}